package com.portfolio.controller;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.servlet.ModelAndView;

import com.portfolio.entity.Sobre;
import com.portfolio.service.FormacaoService;

@Controller
@RequestMapping("/admin/formacao")
public class ControllerFormacao {

	@Autowired
	private FormacaoService service;
	
	@GetMapping
	public ModelAndView home() {
		ModelAndView modelAndView = new ModelAndView("adminFormacao").addObject("sobre", new Sobre());
		return modelAndView;
	}
	
}
