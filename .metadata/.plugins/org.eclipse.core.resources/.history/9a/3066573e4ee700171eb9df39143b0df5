package com.portfolio.service;

import java.io.Serializable;
import java.util.List;

import com.portfolio.entity.Imagem_PR;

public interface Imagem_PRService extends Serializable{

	public List<Imagem_PR> listar();
	
	public Imagem_PR obter(Long id);
	
	public void incluir(Imagem_PR imagem);
	
	public void alterar(Imagem_PR imagem);
	
	public void remover(Long codigoImagem);
	
}